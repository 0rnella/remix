    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "0.0.0-experimental-1ed7e183",
    "@remix-run/eslint-config": "0.0.0-experimental-1ed7e183",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "eslint": "^8.10.0",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
