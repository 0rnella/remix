{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "npm run build && concurrently \"npm:dev:remix\" \"npm:dev:cloudflare\"",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:cloudflare": "cross-env NODE_ENV=development wrangler dev --local --config wrangler.dev.toml",
    "start": "npm run dev:cloudflare"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@remix-run/cloudflare": "*",
    "@remix-run/react": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "miniflare": "^2.6.0",
    "typescript": "^4.8.3",
    "wrangler": "2.1.3"
  },
  "engines": {
    "node": ">=16.13"
  }
}
